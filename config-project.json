{
  "proot": "./",
  "src": "src",
  "dist": "dist",
  "domain": "wordpress.local",
  "wpname": "theme-name",
  "sync": {
    "run": true
  },
  "html": {
    "run": true,
    "pug": true,
    "data": true,
    "inline": true,
    "src": "views",
    "dist": "",
    "minify": false,
    "lint": true
  },
  "wp": {
    "run": false,
    "theme": false,
    "plugin": false,
    "pug": true,
    "pug2blade": false,
    "pug2php": false,
    "sourcemaps": true,
    "lint": true
  },
  "css": {
    "run": true,
    "sass": true,
    "src": "scss",
    "dist": "css",
    "minify": true,
    "autoprefix": true,
    "sourcemaps": true,
    "lint": true
  },
  "js": {
    "run": true,
    "src": "js",
    "dist": "js",
    "uglify": true,
    "sourcemaps": true,
    "lint": true
  },
  "gfx": {
    "run": true,
    "src": "gfx",
    "dist": "gfx"
  },
  "fonts": {
    "run": true,
    "src": "fonts",
    "dist": "fonts"
  },
  "favicon": {
    "run": true
  },
  "critical": {
    "run": true
  },
  "kss": {
    "run": true,
    "dist": "docs/styleguide"
  },
  "sassdoc": {
    "run": true,
    "dist": "docs/sass"
  },
  "jsdoc": {
    "run": true,
    "dist": "docs/js"
  },
  "bump": {
    "run": true
  },
  "gzip": {
    "run": false
  },
  "yarn": false
}
